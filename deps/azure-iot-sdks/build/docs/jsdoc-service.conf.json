{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": [
      "common/core/common.js",
      "common/core/lib/errors.js",
      "common/core/lib/message.js",
      "common/core/lib/shared_access_signature.js",
      "common/transport/amqp/index.js",
      "common/transport/amqp/lib/amqp.js",
      "common/transport/amqp/lib/amqp_message.js",
      "common/transport/amqp/lib/amqp_receiver.js",
      "common/transport/http/index.js",
      "common/transport/http/lib/http.js",
      "service/iothub.js",
      "service/lib/amqp.js",
      "service/lib/client.js",
      "service/lib/device.js",
      "service/lib/registry.js"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "default": {
        "useLongnameInNav": true
    },
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "markdown": {
    "tags": [
      "module"
    ]
  },
  "opts": {
    "template": "../build/docs/iot-doc-template"
  }
}